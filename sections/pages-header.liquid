<div class="position-relative text-center p-3 p-md-5 brand-template__background">
  <div class="col-md-5 mx-auto my-5">
    <h1>
      {{ section.settings.title }}</h1>
    <p class="lead">
      {{ section.settings.paragraph1 }}
    </p>
    <p>{{ section.settings.paragraph2 }}
    </p>
  </div>
</div>




{% schema %}
  {
    "name": "Page Header",
    "settings": [
      {
        "type": "text",
        "id": "title",
        "label": "Heading"
      }, {
        "type": "image_picker",
        "id": "image",
        "label": "Background Image"
      }, {
        "type": "richtext",
        "id": "paragraph1",
        "label": "first paragraph1"
      }, {
        "type": "text",
        "id": "paragraph2",
        "label": "second paragraph1"
      }
    ],
    "presets": [
      {
        "category": "Page Header",
        "name": "Page Header"
      }
    ]
  }
{% endschema %}